<div class="well well-lg">
	<div class="row">
		<div class="col-xs-12">
			<ol class="breadcrumb">
			  <li><a href="/u/<%= req.user.id %>/dashboard">Dashboard</a></li>
			  <li><a href="/c/<%= tournament.community.id %>"><%= tournament.community.name %></a></li>
			  <li class="active"><%= tournament.name %></li>
			</ol>
		</div>
		<div class="col-xs-12">
			<h2><span class="underline-green"><%= tournament.name %></span></h2>
			<div class="row">
				<div class="col-xs-12">
				<table class="table">
					<thead>
						<tr>
							<td>Placing</td>
							<td>Smasher</td>
						</tr>
					</thead>
					<tbody>
						<% for(var i = 0; i < tournament.placings.length; i++) { %>
							<tr>
								<td><%= tournament.placings[i].place %></td>
								<td><%= tournament.placings[i].smasher.tag %></td>
							</tr>
						<% } %>
					</tbody>
				</table>
				</div>
			</div>
		</div>
	</div>
</div>
